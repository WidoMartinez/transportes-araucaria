{
  "framework": "vite",
  "nodeVersion": "22.x",
  "buildDirectory": "backend",
  "buildCommand": "cd backend && npm install",
  "startCommand": "cd backend && npm run start:migrate",
  "environmentVariables": [
    "DB_HOST",
    "DB_USER",
    "DB_PASSWORD",
    "DB_NAME",
    "DB_PORT",
    "JWT_SECRET",
    "PORT",
    "NODE_ENV",
    "ALLOWED_ORIGINS"
  ]
}
